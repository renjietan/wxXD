
.header {
    margin: 90rpx 0 90rpx 50rpx;
    border-bottom: 1px solid #ccc;
    text-align: center;
    width: 650rpx;
    height: 300rpx;
    line-height: 450rpx;
}
 
.header image {
    width: 200rpx;
    height: 200rpx;
}
 
.content {
    margin-left: 50rpx;
    margin-bottom: 90rpx;
}
 
.content text {
    display: block;
    color: #9d9d9d;
    margin-top: 40rpx;
}
 
.bottom {
    border-radius: 80rpx;
    margin: 70rpx 50rpx;
    font-size: 35rpx;
}

.warp{
  min-height: 100vh;
  padding: 20rpx;
  box-sizing: border-box;
  background: #efeff4;
}
.warp text{
  display: block;
  background-color: #fff;
  border-radius: 8rpx;
  box-shadow: 0 2rpx 9rpx rgba(26, 183, 249, 0.13);
  line-height: 180rpx;
  color:#333;
  font-size:30rpx;
  text-align: center;
}