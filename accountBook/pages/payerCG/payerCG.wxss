.content_box_handle_btn{
  width: 120rpx !important;
  height: 52rpx  !important;
  line-height: 52rpx !important;
  border:2rpx solid #cccccc !important;
  border-radius: 26rpx !important;
  background: #fff !important;
  color: #cccccc !important;
  box-shadow: 0 0 2rpx 1rpx #cccccc !important
}

.move {
  position: fixed;
  width: calc(100% - 40rpx);
  right: 20rpx;
  bottom: 150rpx;
}

.addbtn {
  width: 90rpx;
  height: 90rpx;
  background-color: #3a93ff;
  box-shadow: 0px 1rpx 18rpx 0rpx rgba(3, 84, 181, 0.43);
  border-radius: 90rpx;
  text-align: center;
  line-height: 90rpx;
  margin-left: calc(100% - 90rpx);
}

.addbtn i {
  width: 55rpx;
  height: 55rpx;
  font-size: 55rpx;
  color: #fff;
}
